{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "name": "myles-wiki",
    "framework": "vuepress",
    "buildCommand": "vuepress build .",
    "outputDirectory": ".vuepress/dist/",
    "devCommand": "vuepress dev .",
    "installCommand": "npm install --production",
    "rewrites": [
        {
            "source": "/assets/:path*",
            "destination": "https://media.githubusercontent.com/media/myles/wiki/master/assets/:path*"
        }
    ]
}
