{
  "root": "",
  "title": "Myles' Wiki",
  "author": "Myles Braithwaite",
  "plugins": [
    "algolia",
    "edit-link",
    "rss",
    "todo",
    "json",
    "changelog"
  ],
  "pluginsConfig": {
    "dateFormat": {
      "dateFormat": "Do MMMM YYYY, h:mm:ss a",
      "files": [
        ".bookignore",
        ".gitattributes",
        ".gitignore",
        ".remarkrc",
        ".travis.yml",
        "book.json",
        "netlify.toml",
        "package-lock.json",
        "package.json",
        "renovate.json"
      ]
    },
    "algolia": {
      "index": "documents",
      "applicationID": "PMGLYE37G3",
      "publicKey": "5de5c3bc27e84a4b30f7d90d10b9245c",
      "freeAccount": "true"
    },
    "edit-link": {
      "base": "https://github.com/myles/wiki/edit/master/",
      "label": "Edit"
    },
    "rss": {
      "title": "Myles' Wiki",
      "author": "Myles Braithwaite",
      "site_url": "https://wiki.mylesb.ca/",
      "feed_url": "feed.xml"
    },
    "json": {
      "output": "_book/book.json"
    },
    "autocover": {
      "font": {
        "size": null,
        "family": "Impact",
        "color": "#fff"
      },
      "size": {
        "w": "1800",
        "h": "2360"
      },
      "background": {
        "color": "#09f"
      }
    }
  }
}
